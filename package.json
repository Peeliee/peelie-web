{
    "name": "peelie",
    "version": "1.0.0",
    "private": true,
    "packageManager": "pnpm@10.10.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:web": "pnpm --filter peelie-web dev",
        "dev:app": "pnpm --filter peelie-app start",
        "build:web": "pnpm --filter peelie-web build"
    }
}
